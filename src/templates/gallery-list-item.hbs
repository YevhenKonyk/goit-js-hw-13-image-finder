<li class="gallery_item">
    <div class="photo-card">
        <img src="{{webformatURL}}" data-full-image="{{largeImageURL}}"
            alt="image" width="400" />

        <div class="stats">
            <p class="stats-item">
                <i class="material-icons">thumb_up</i>
                {{likes}}
            </p>
            <p class="stats-item">
                <i class="material-icons">visibility</i>
                {{views}}
            </p>
            <p class="stats-item">
                <i class="material-icons">comment</i>
                {{comments}}
            </p>
            <p class="stats-item">
                <i class="material-icons">cloud_download</i>
                {{downloads}}
            </p>
        </div>
        <ul class="tags">
            {{#each tags}}
            <li class="tags-item">{{this}}</li>
            {{/each}}
        </ul>
    </div>
</li>

{{!-- webformatURL, largeImageURL, likes, views, comments, downloads --}}